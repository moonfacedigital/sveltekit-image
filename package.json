{
  "name": "sveltekit-image",
  "version": "0.4.3",
  "author": {
    "name": "Moonface Digital",
    "email": "dev@moonface.digital",
    "url": "https://moonface.digital"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moonfacedigital/sveltekit-image.git"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./api": {
      "types": "./dist/api/requestHandler.d.ts",
      "svelte": "./dist/api/requestHandler.js"
    }
  },
  "scripts": {
    "dev": "vite dev",
    "start": "vite dev",
    "build": "svelte-kit sync && svelte-package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check .",
    "format": "prettier --plugin-search-dir . --write .",
    "build:web": "vite build",
    "pub": "pnpm build && pnpm version patch --force && pnpm publish --no-git-checks"
  },
  "type": "module",
  "dependencies": {
    "@hapi/accept": "^6.0.1",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/adapter-netlify": "^2.0.5",
    "@sveltejs/kit": "^1.10.0",
    "@sveltejs/package": "^2.0.2",
    "@types/send": "^0.17.1",
    "@types/sharp": "^0.31.1",
    "is-animated": "^2.0.2",
    "prettier": "^2.8.4",
    "prettier-plugin-svelte": "^2.9.0",
    "send": "^0.18.0",
    "sharp": "^0.31.3",
    "svelte": "^3.55.1",
    "svelte-adapter-bun": "^0.4.3",
    "svelte-check": "^3.0.4",
    "svelte-preprocess": "^5.0.1",
    "sveltekit-image": "^0.4.2",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.4"
  }
}
